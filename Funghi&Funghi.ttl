@prefix : <http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix efo: <http://www.ebi.ac.uk/efo/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix pato: <http://purl.obolibrary.org/obo/pato#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix chebi: <http://purl.obolibrary.org/obo/chebi/> .
@prefix swrla: <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix foodOn: <http://purl.obolibrary.org/obo/foodon.owl#> .
@prefix schema: <http://schema.org/> .
@prefix oboInOwl: <http://www.geneontology.org/formats/oboInOwl#> .
@prefix collectionentity: <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@base <http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#> .

<http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21> rdf:type owl:Ontology ;
                                                                           owl:imports <http://www.ontologydesignpatterns.org/cp/owl/bag.owl> ;
                                                                           dc:creator "Andrea Fancellu"@it ;
                                                                           dc:date "25/04/2024" ;
                                                                           dc:description "L'Ontologia Funghi&Funghi è stata realizzata per modellare il dominio dei funghi in Gallura, subregione della Sardegna. Lo scopo dell'ontologia è quello di fornire un modello consultabile per discernere i funghi commestibili da quelli non commestibili."@it ;
                                                                           dc:title "Funghi&Funghi"@it ;
                                                                           vann:preferredNamespacePrefix "funghi&funghi" ;
                                                                           vann:preferredNamespaceUri "http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21"^^xsd:anyURI ;
                                                                           rdfs:label "Ontologia per i Funghi della Gallura"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#abitato_da
:abitato_da rdf:type owl:ObjectProperty ;
            owl:inverseOf :haHabitat ;
            rdfs:domain :Habitat ;
            rdfs:range :Fungo ;
            rdfs:comment "proprietà che indica quale fungo abita un certo habitat"@it ;
            rdfs:label "abitato da"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#confondibileCon
:confondibileCon rdf:type owl:ObjectProperty ,
                          owl:TransitiveProperty ;
                 rdfs:domain :Fungo ;
                 rdfs:range :Fungo ;
                 rdfs:comment "relazione che specifica quali funghi assomigliano a quali funghi"@it ;
                 rdfs:label "confondibile con"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#contiene_fungo
:contiene_fungo rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf bag:itemContent ;
                rdfs:domain :Fungo_Raccolto ;
                rdfs:range :Fungo ;
                rdfs:comment "prorpietà che rappresenta l'appartenenza del fungo al place holder nel bag"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#cresce_durante
:cresce_durante rdf:type owl:ObjectProperty ;
                owl:inverseOf :durante_cresce ;
                rdfs:domain :Fungo ;
                rdfs:range :Stagione ;
                rdfs:comment "proprietà che indica quale fungo cresce in quale stagione"@it ;
                rdfs:label "cresce durante"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#durante_cresce
:durante_cresce rdf:type owl:ObjectProperty ;
                rdfs:domain :Stagione ;
                rdfs:range :Fungo ;
                rdfs:comment "Proprietà che indica durante quale stagione cresce quale fungo"@it ;
                rdfs:label "durante cresce"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#fungoSiTrovaIn
:fungoSiTrovaIn rdf:type owl:ObjectProperty ;
                rdfs:domain :Fungo ;
                rdfs:range :Regione ;
                owl:propertyChainAxiom ( :haHabitat
                                         :siTrovaIn
                                       ) ;
                rdfs:comment "relazione che specifica in che regione cresce un determinato fungo."@it ;
                rdfs:label "cresce in"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#haHabitat
:haHabitat rdf:type owl:ObjectProperty ;
           rdfs:domain :Fungo ;
           rdfs:range :Habitat ;
           rdfs:comment "Proprietà che specifica l'habitat di un determinato fungo"@it ;
           rdfs:label "ha habitat"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#haParassita
:haParassita rdf:type owl:ObjectProperty ;
             owl:inverseOf :parassitaDi ;
             rdfs:domain :Albero ;
             rdfs:range :Fungo_Parassita ;
             rdfs:comment "proprietà che specifica qual è il fungo parassita di un determinato albero"@it ;
             rdfs:label "ha parassita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#haSaprofita
:haSaprofita rdf:type owl:ObjectProperty ;
             owl:inverseOf :saprofitaDi ;
             rdfs:domain :Albero ;
             rdfs:range :Fungo_Saprofita ;
             rdfs:comment "proprietà che specifica qual è il fungo saprofita di un determinato albero"@it ;
             rdfs:label "ha saprofita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#haSimbionte
:haSimbionte rdf:type owl:ObjectProperty ;
             owl:inverseOf :simbionteDi ;
             rdfs:domain :Albero ;
             rdfs:range :Fungo_Simbionte ;
             rdfs:comment "proprietà che specifica qual è il fungo simbionte di un determinato albero"@it ;
             rdfs:label "ha simbionte"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#ha_cestino
:ha_cestino rdf:type owl:ObjectProperty ;
            rdfs:domain :Persona ;
            rdfs:range :Cestino ;
            rdfs:comment "Proprietà che indica l'appartenenza di un cestino a un raccoglitore"@it ;
            rdfs:label "possiede"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#mangia
:mangia rdf:type owl:ObjectProperty ;
        owl:inverseOf :mangiato_da ;
        rdfs:domain :Persona ;
        rdfs:range :Fungo ;
        rdfs:comment "proprietà che rappresenta il fatto che un raccoglitore mangia un fungo"@it ;
        rdfs:label "mangia"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#mangiato_da
:mangiato_da rdf:type owl:ObjectProperty ;
             rdfs:domain :Fungo ;
             rdfs:range :Persona ;
             rdfs:comment "proprietà che rappresenta da chi viene mangiato un determinato fungo"@it ;
             rdfs:label "mangiato da"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#parassitaDi
:parassitaDi rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Fungo_Parassita ;
             rdfs:range :Albero ;
             rdfs:comment "Proprietà che specifica di quale albero è parassita un fungo parassita"@it ;
             rdfs:label "parassita di"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#raccoglie
:raccoglie rdf:type owl:ObjectProperty ;
           owl:inverseOf :raccoltoDa ;
           rdfs:domain :Persona ;
           rdfs:range :Fungo ;
           rdfs:comment "Proprietà che specifica quale fungo raccoglie una certa persona"@it ;
           rdfs:label "raccoglie"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#raccoltoDa
:raccoltoDa rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Fungo ;
            rdfs:range :Persona ;
            rdfs:comment "Proprietà che indica da chi è stato raccolto un determinato fungo"@it ;
            rdfs:label "raccolto da"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#saprofitaDi
:saprofitaDi rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Fungo_Saprofita ;
             rdfs:range :Albero ;
             rdfs:comment "Proprietà che specifica di quale albero è saprofita un fungo saprofita"@it ;
             rdfs:label "saprofita di"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#siTrovaIn
:siTrovaIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Habitat ;
           rdfs:range :Regione ;
           rdfs:comment "Proprietà che specifica in quale regione si trova un certo habitat"@it ;
           rdfs:label "si trova in"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#simbionteDi
:simbionteDi rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Fungo_Simbionte ;
             rdfs:range :Albero ;
             rdfs:comment "Proprietà che specifica di quale albero è simbionte un fungo simbionte"@it ;
             rdfs:label "simbionte di"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#studia
:studia rdf:type owl:ObjectProperty ;
        owl:inverseOf :studiato_da ;
        rdfs:domain :Persona ;
        rdfs:range :Fungo ;
        rdfs:comment "rappresenta il fatto che un fungo sia studiaatod a un raccoglitore. Chi studia un fungo è un micologo. Un micologo non può essere inesperto."@it ;
        rdfs:label "studia"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#studiato_da
:studiato_da rdf:type owl:ObjectProperty ;
             rdfs:domain :Fungo ;
             rdfs:range :Persona ;
             rdfs:comment "proprietà che rappresenta il fatto che un fungo venga studiato da un determinato raccoglitore"@it ;
             rdfs:label "studiato da"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#colore
:colore rdf:type owl:DatatypeProperty ;
        rdfs:domain :Fungo ;
        rdfs:range xsd:string ;
        rdfs:comment "Proprietà che indica il colore del cappello di un determinato fungo"@it ;
        rdfs:label "colore"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#colore_cappello
:colore_cappello rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :colore ;
                 rdfs:domain :Fungo ;
                 rdfs:range xsd:string ;
                 rdfs:comment "proprietà che rappresenta il colore del gambo del fungo"@it ;
                 rdfs:label "colore gambo"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#colore_gambo
:colore_gambo rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf :colore ;
              rdfs:domain :Fungo ;
              rdfs:range xsd:string ;
              rdfs:comment "proprietà che rappresenta il color del cappello del fungo"@it ;
              rdfs:label "colore cappello"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#ha_lamelle
:ha_lamelle rdf:type owl:DatatypeProperty ;
            rdfs:domain :Fungo ;
            rdfs:range xsd:boolean ;
            owl:propertyDisjointWith :ha_tubuli ;
            rdfs:comment "proprietà che rappresenta la presenza o assenza di lamelle nella parte inferiore del cappello"@it ;
            rdfs:label "ha lamelle"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#ha_nome_fungo
:ha_nome_fungo rdf:type owl:DatatypeProperty ;
               rdfs:domain :Fungo ;
               rdfs:range xsd:string ;
               rdfs:comment "proprietà che rappresenta il nome di un fungo"@it ;
               rdfs:label "ha nome fungo"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#ha_nome_raccoglitore
:ha_nome_raccoglitore rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Persona ;
                      rdfs:range xsd:string ;
                      rdfs:comment "Proprietà che rappresenta il nome di un raccoglitore"@it ;
                      rdfs:label "ha nome raccoglitore"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#ha_tubuli
:ha_tubuli rdf:type owl:DatatypeProperty ;
           rdfs:domain :Fungo ;
           rdfs:range xsd:boolean ;
           rdfs:comment "proprietà che rappressenta la presenza o assenza di tubuli nella parte inferiore del cappello del fungo"@it ;
           rdfs:label "ha tubuli"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#peso
:peso rdf:type owl:DatatypeProperty ;
      rdfs:domain :Fungo ;
      rdfs:range xsd:positiveInteger ;
      rdfs:comment "Proprietà che idnica il peso espresso in grammi di un determinato fungo"@it ;
      rdfs:label "peso"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#sano
:sano rdf:type owl:DatatypeProperty ;
      rdfs:domain :Fungo ;
      rdfs:range xsd:boolean ;
      rdfs:comment "proprietà che indica se un fungo è o meno sano. Un fungo è definito sano se è privo di parassiti, come insetti o patogeni vari."@it ;
      rdfs:label "sano"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#velenoso
:velenoso rdf:type owl:DatatypeProperty ;
          rdfs:domain :Fungo ;
          rdfs:range xsd:boolean ;
          rdfs:comment "proprietà che determina la velenosità di un fungo"@it ;
          rdfs:label "velenoso"@it .


#################################################################
#    Classes
#################################################################

###  http://purl.obolibrary.org/obo/FOODON_03412735
obo:FOODON_03412735 rdf:type owl:Class ;
                    owl:equivalentClass :Armillaria .


###  http://purl.obolibrary.org/obo/NCBITaxon_40144
obo:NCBITaxon_40144 rdf:type owl:Class ;
                    owl:equivalentClass :Tricholoma .


###  http://purl.obolibrary.org/obo/NCBITaxon_41955
obo:NCBITaxon_41955 rdf:type owl:Class ;
                    owl:equivalentClass :Amanita .


###  http://purl.obolibrary.org/obo/NCBITaxon_5204
obo:NCBITaxon_5204 rdf:type owl:Class .


###  http://purl.obolibrary.org/obo/NCBITaxon_5369
obo:NCBITaxon_5369 rdf:type owl:Class ;
                   owl:equivalentClass :Boletus .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Albero
:Albero rdf:type owl:Class ;
        rdfs:comment "Classe che rappresenta il concetto albero, inteso come struttura che favorisce la crescita di funghi simbionti, saprofiti e parassiti"@it ;
        rdfs:label "Albero"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Amanita
:Amanita rdf:type owl:Class ;
         rdfs:subClassOf :Fungo_Simbionte ;
         rdfs:comment "Amanita rappresenta un genere di funghi velenosi o commestibili della famiglia Amanitaceae. Sono caratterizzati da cappelli ampi, lamelle e anello sul gambo, residuo dello stato primordiale del corpo fruttifero. Alcune specie producono tossine pericolose per l'uomo."@it ;
         rdfs:label "Amanita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Amanita_Caesarea
:Amanita_Caesarea rdf:type owl:Class ;
                  rdfs:subClassOf :Amanita ;
                  rdfs:comment "Caesarea rappresenta una specie di fungo commestibile appartenente al genere Amanita. Questo fungo è caratterizzato da un cappello arancione, lamelle bianche e gambo bianco, ed è altamente apprezzato per il suo gusto delicato e la sua consistenza."@it ;
                  rdfs:label "Caesarea"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Amanita_Muscaria
:Amanita_Muscaria rdf:type owl:Class ;
                  rdfs:subClassOf :Amanita ;
                  rdfs:comment "Muscaria rappresenta una specie di fungo velenoso appartenente al genere Amanita. Questo fungo è caratterizzato da un cappello rosso vivo punteggiato di bianco, lamelle bianche e gambo bianco striato, ed è noto per la presenza di sostanze psicoattive."@it ;
                  rdfs:label "Muscaria"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Amanita_Pantherina
:Amanita_Pantherina rdf:type owl:Class ;
                    rdfs:subClassOf :Amanita ;
                    rdfs:comment "Pantherina rappresenta una specie di fungo velenoso appartenente al genere Amanita. Questo fungo è caratterizzato da un cappello marrone chiaro con macchie bianche, lamelle bianche e gambo bianco o crema, ed è considerato altamente tossico per l'uomo."@it ;
                    rdfs:label "Pantherina"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Amanita_Phalloides
:Amanita_Phalloides rdf:type owl:Class ;
                    rdfs:subClassOf :Amanita ;
                    rdfs:comment "Phalloides rappresenta una specie di fungo altamente velenoso appartenente al genere Amanita. Questo fungo è caratterizzato da un cappello verde oliva, lamelle bianche e gambo bianco o verdastro, ed è noto per contenere potenti sostanze tossiche che possono causare danni gravi al fegato e persino la morte."@it ;
                    rdfs:label "Phalloides"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Amanita_Verna
:Amanita_Verna rdf:type owl:Class ;
               rdfs:subClassOf :Amanita ;
               rdfs:comment "Verna rappresenta una specie di fungo velenoso appartenente al genere Amanita. Questo fungo è caratterizzato da un cappello bianco o crema, lamelle bianche e gambo bianco, ed è noto per contenere sostanze tossiche mortali per l'uomo."@it ;
               rdfs:label "Verna"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Armillaria
:Armillaria rdf:type owl:Class ;
            rdfs:subClassOf :Fungo_Parassita ;
            rdfs:comment "Armillaria rappresenta un genere di funghi parassiti della famiglia Physalacriaceae. I funghi Armillaria sono caratterizzati da cappelli convessi, lamelle e anello sul gambo."@it ;
            rdfs:label "Armillaria"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Armillaria_Mellea
:Armillaria_Mellea rdf:type owl:Class ;
                   rdfs:subClassOf :Armillaria ;
                   rdfs:comment "Mellea rappresenta una specie di fungo parassita appartenente al genere Armillaria. Questo fungo è caratterizzato da un cappello marrone scuro, lamelle bianche e gambo fibroso, ed è noto per causare marciume radicale nelle piante ospiti. È comunemente noto come \"chiodino\" per il fatto che sviluppi più carpofori che crescono in gruppo e per la sua forma."@it ;
                   rdfs:label "Mellea"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Boletus
:Boletus rdf:type owl:Class ;
         rdfs:subClassOf :Fungo_Simbionte ;
         rdfs:comment "Classe rappresentante i Boletus, classe di funghi caratterizzate da tubuli al posto delle lamesse nella parte inferiore del cappello."@it ;
         rdfs:label "Boletus"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Boletus_Aestivalis
:Boletus_Aestivalis rdf:type owl:Class ;
                    rdfs:subClassOf :Boletus ;
                    rdfs:comment "Aestivalis rappresenta una specie di fungo porcino appartenente al genere Boletus, presente nell'ecosistema della Sardegna. Questo fungo è conosciuto per il suo cappello marrone chiaro, tubuli gialli e gambo slanciato, ed è apprezzato per il suo sapore delicato e la sua consistenza carnosa."@it ;
                    rdfs:label "Aestivalis"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Boletus_Edulis
:Boletus_Edulis rdf:type owl:Class ;
                rdfs:subClassOf :Boletus ;
                rdfs:comment "Edulis rappresenta una delle specie di fungo più rinomate e ricercate, appartenente al genere Boletus. Questo fungo è caratterizzato da un cappello marrone scuro, tubuli bianchi o gialli e gambo massiccio, ed è ampiamente apprezzato per il suo gusto ricco e aromatico."@it ;
                rdfs:label "Edulis"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Boletus_Satanas
:Boletus_Satanas rdf:type owl:Class ;
                 rdfs:subClassOf :Boletus ;
                 rdfs:comment "Satanas rappresenta una specie di fungo velenoso appartenente al genere Boletus. Questo fungo è caratterizzato da un cappello marrone con sfumature rosse, tubuli gialli e gambo massiccio e rosso, ed è noto per contenere sostanze tossiche che possono causare gravi disturbi gastro-intestinali e reazioni avverse in caso di ingestione."@it ;
                 rdfs:label "Satanas"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Cestino
:Cestino rdf:type owl:Class ;
         rdfs:subClassOf bag:Bag ;
         rdfs:comment "Classe che rappresenta il cestino di funghi di un raccoglitore"@it ;
         rdfs:label "Cestino"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Con_Parassita
:Con_Parassita rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :haParassita ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Fungo_Parassita
                                   ] ;
               rdfs:subClassOf :Albero ;
               rdfs:comment "Albero che ospita almeno 1 parassita"@it ;
               rdfs:label "ConParassita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Con_Saprofita
:Con_Saprofita rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :haSaprofita ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Fungo_Saprofita
                                   ] ;
               rdfs:subClassOf :Albero ;
               rdfs:comment "Albero che ospita almeno 1 saprofita"@it ;
               rdfs:label "ConSaprofita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Con_Simbionte
:Con_Simbionte rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :haSimbionte ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Fungo_Simbionte
                                   ] ;
               rdfs:subClassOf :Albero ;
               rdfs:comment "Alberp che ospita almeno 1 simbionte"@it ;
               rdfs:label "ConSimbionte"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo
:Fungo rdf:type owl:Class ;
       rdfs:subClassOf obo:NCBITaxon_5204 ;
       rdfs:comment "Fungo rappresenta un organismo del regno Fungi, presente nell'ecosistema della Sardegna. I funghi possono variare in termini di forma, dimensioni, peso, colore, e possono essere commestibili o velenosi."@it ;
       rdfs:label "Fungo"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo_Commestibile
:Fungo_Commestibile rdf:type owl:Class ;
                    rdfs:subClassOf :Fungo ;
                    rdfs:comment "Classe che indica un fungo comemsstibile, inteso come un fungo non velenoso."@it ;
                    rdfs:label "Commestibile"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo_Comune
:Fungo_Comune rdf:type owl:Class ;
              rdfs:subClassOf :Fungo ;
              rdfs:comment "Un fungo comune è un fungo che cresce in almeno 2 habitat differenti"@it ;
              rdfs:label "Comune"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo_Parassita
:Fungo_Parassita rdf:type owl:Class ;
                 rdfs:subClassOf :Fungo ;
                 rdfs:comment "Parassita rappresenta un tipo di fungo che si nutre di altre specie, causando danni o malattie. Questi funghi possono infettare piante o altri organismi, compromettendo la loro salute e la loro sopravvivenza."@it ;
                 rdfs:label "Parassita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo_Raccolto
:Fungo_Raccolto rdf:type owl:Class ;
                rdfs:subClassOf bag:Item ;
                rdfs:comment "classe che rappresenta un fungo raccolto da un raccoglitore"@it ;
                rdfs:label "Fungo Raccolto"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo_Saprofita
:Fungo_Saprofita rdf:type owl:Class ;
                 rdfs:subClassOf :Fungo ;
                 rdfs:comment "Saprofita rappresenta un tipo di fungo che si nutre di materiale organico morto o in decomposizione. Questi funghi svolgono un ruolo fondamentale nella decomposizione della materia organica, contribuendo al riciclaggio dei nutrienti nell'ecosistema sardo."@it ;
                 rdfs:label "Saprofita"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Fungo_Simbionte
:Fungo_Simbionte rdf:type owl:Class ;
                 rdfs:subClassOf :Fungo ;
                 rdfs:comment "Simbionte rappresenta un tipo di fungo che stabilisce una relazione simbiotica con altre specie. Questi funghi forneiscono benefici sia al fungo stesso che all'organismo ospite. La simbiosi fungina è cruciale per la salute degli ecosistemi sardi, contribuendo alla nutrizione e alla resistenza delle piante."@it ;
                 rdfs:label "Simbionte"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Habitat
:Habitat rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Class ;
                               owl:oneOf ( :Collina
                                           :Mare
                                           :Montagna
                                           :Pianura
                                         )
                             ] ;
         rdfs:comment "Habitat rappresenta il tipo di ambiente in cui si sviluppano i funghi in Sardegna. Gli habitat fungini possono variare da foreste di conifere ad ambienti di macchia mediterranea, da prati umidi ad ambienti montani."@it ;
         rdfs:label "Habitat"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Inesperto
:Inesperto rdf:type owl:Class ;
           rdfs:subClassOf :Persona ;
           rdfs:comment "Inesperto rappresenta un raccoglitore che raccoglie anche funghi velenosi"@it ;
           rdfs:label "Inesperto"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Micologo
:Micologo rdf:type owl:Class ;
          rdfs:subClassOf :Persona ;
          rdfs:comment "Un micologo è un raccoglitore che studia i funghi"@it ;
          rdfs:label "Micologo"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Persona
:Persona rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :raccoglie ;
                               owl:someValuesFrom :Fungo
                             ] ;
         rdfs:subClassOf foaf:Person ;
         rdfs:comment "Raccoglitore rappresenta un individuo che si diletta nella raccolta dei funghi in Sardegna."@it ;
         rdfs:label "Raccoglitore"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Raccoglitore_Esperto
:Raccoglitore_Esperto rdf:type owl:Class ;
                      rdfs:subClassOf :Persona ;
                      rdfs:comment "Raccoglitore che trova un fungo di almeno 1 kg"@it ;
                      rdfs:label "Fortunato"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Regione
:Regione rdf:type owl:Class ;
         rdfs:comment "Regione rappresenta una suddivisione della Sardegna, caratterizzata da specifiche caratteristiche geografiche, culturali e ambientali."@it ;
         rdfs:label "Regione"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Stagione
:Stagione rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( :Autunno
                                            :Estate
                                            :Inverno
                                            :Primavera
                                          )
                              ] ;
          rdfs:comment "Stagione rappresenta le quattro stagioni dell'anno in cui possono crescere funghi differenti."@it ;
          rdfs:label "Stagione"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Tricholma_Portentosum
:Tricholma_Portentosum rdf:type owl:Class ;
                       rdfs:subClassOf :Tricholoma ;
                       owl:disjointWith :Tricholoma_Pardinum ;
                       rdfs:comment "Portentosum rappresenta una specie di fungo appartenente al genere Tricholoma, presente nell'ecosistema della Sardegna. Particolarmente apprezzato come fungo commestibile, si distingue per il colore marrone/grigio scuro con riflessi e sfumature gialle su cappello, gambo e carne."@it ;
                       rdfs:label "Portentosum"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Tricholoma
:Tricholoma rdf:type owl:Class ;
            rdfs:subClassOf :Fungo_Saprofita ;
            rdfs:comment "Tricholoma rappresenta un genere di funghi commestibili o tossici della famiglia Tricholomataceae. Sono caratterizzati da cappelli convessi, lamelle libere e gambo fibroso, e alcune specie sono apprezzate come alimento."@it ;
            rdfs:label "Tricholoma"@it .


###  http://www.semanticweb.org/andre/ontologies/2024/3/untitled-ontology-21#Tricholoma_Pardinum
:Tricholoma_Pardinum rdf:type owl:Class ;
                     rdfs:subClassOf :Tricholoma ;
                     rdfs:comment "Pardinum rappresenta una specie di fungo velenoso appartenente al genere Tricholoma. Questo fungo è caratterizzato da un cappello marrone chiaro con macchie grigie o brune, lamelle bianche e gambo bianco o grigiastro, ed è considerato tossico per l'uomo."@it ;
                     rdfs:label "Pardinum"@it .


